# Telegram API credentials - Get these from https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Your phone number (with country code, e.g., +1234567890)
TELEGRAM_PHONE=your_phone_number_here

# Optional: Set to 'development' for more verbose logging
NODE_ENV=production

# Optional: Custom session directory (defaults to ./session)
SESSION_DIR=./session
